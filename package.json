{
    "name": "bsky-event-handlers",
    "version": "0.4.59",
    "description": "Utilities to help handle bluesky events with validation",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "test": "jest",
        "coverage": "jest --coverage",
        "prepublishOnly": "npm run build",
        "format": "npx prettier '{src,tests}/**/*.{ts,js}' --write",
        "lint": "eslint --ext=.ts src/ tests/ --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/juni-b-queer/bsky-event-handlers.git"
    },
    "keywords": [
        "bluesky"
    ],
    "author": "Juniper Bender",
    "license": "BSD-4-Clause",
    "bugs": {
        "url": "https://github.com/juni-b-queer/bsky-event-handlers/issues"
    },
    "homepage": "https://github.com/juni-b-queer/bsky-event-handlers#readme",
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^11.1.0",
        "@semantic-release/exec": "^6.0.3",
        "@types/jest": "^29.5.11",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^7.5.0",
        "@typescript-eslint/parser": "^7.5.0",
        "conventional-changelog-conventionalcommits": "^7.0.2",
        "dotenv": "^16.4.5",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "jest-date-mock": "^1.0.8",
        "jest-mock-extended": "^3.0.5",
        "jest-websocket-mock": "^2.5.0",
        "mock-socket": "^9.3.1",
        "prettier": "3.2.4",
        "semantic-release": "^22.0.12",
        "ts-jest": "^29.2.0",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@atproto/api": "^0.12.22",
        "atproto-firehose": "^0.2.2",
        "ws": "^8.16.0"
    },
    "release": {
        "plugins": [
            [
                "@semantic-release/commit-analyzer",
                {
                    "preset": "conventionalcommits",
                    "releaseRules": [
                        {
                            "type": "breaking",
                            "release": "major"
                        },
                        {
                            "type": "major",
                            "release": "major"
                        },
                        {
                            "type": "feat",
                            "release": "minor"
                        },
                        {
                            "type": "feature",
                            "release": "minor"
                        },
                        {
                            "type": "minor",
                            "release": "minor"
                        },
                        {
                            "type": "patch",
                            "release": "patch"
                        },
                        {
                            "type": "fix",
                            "release": "patch"
                        },
                        {
                            "type": "formatting",
                            "release": "patch"
                        },
                        {
                            "type": "docs",
                            "release": "patch"
                        },
                        {
                            "type": "refactor",
                            "release": "patch"
                        },
                        {
                            "type": "reorganize",
                            "release": "patch"
                        },
                        {
                            "type": "test",
                            "release": "patch"
                        }
                    ],
                    "presetConfig": {
                        "types": [
                            {
                                "type": "major",
                                "section": "Breaking Changes"
                            },
                            {
                                "type": "breaking",
                                "section": "Breaking Changes"
                            },
                            {
                                "type": "feat",
                                "section": "Features"
                            },
                            {
                                "type": "feature",
                                "section": "Features"
                            },
                            {
                                "type": "minor",
                                "section": "General Updates"
                            },
                            {
                                "type": "patch",
                                "section": "Patches"
                            },
                            {
                                "type": "fix",
                                "section": "Fixes"
                            },
                            {
                                "type": "formatting",
                                "section": "Formatting"
                            },
                            {
                                "type": "docs",
                                "section": "Docs"
                            },
                            {
                                "type": "refactor",
                                "section": "Refactoring"
                            },
                            {
                                "type": "reorganize",
                                "section": "Reorganizing"
                            },
                            {
                                "type": "test",
                                "section": "Testing"
                            }
                        ]
                    }
                }
            ],
            [
                "@semantic-release/release-notes-generator",
                {
                    "preset": "conventionalcommits",
                    "presetConfig": {
                        "types": [
                            {
                                "type": "major",
                                "section": "Breaking Changes"
                            },
                            {
                                "type": "breaking",
                                "section": "Breaking Changes"
                            },
                            {
                                "type": "feat",
                                "section": "Features"
                            },
                            {
                                "type": "feature",
                                "section": "Features"
                            },
                            {
                                "type": "minor",
                                "section": "General Updates"
                            },
                            {
                                "type": "patch",
                                "section": "Patches"
                            },
                            {
                                "type": "fix",
                                "section": "Fixes"
                            },
                            {
                                "type": "formatting",
                                "section": "Formatting"
                            },
                            {
                                "type": "docs",
                                "section": "Docs"
                            },
                            {
                                "type": "refactor",
                                "section": "Refactoring"
                            },
                            {
                                "type": "reorganize",
                                "section": "Reorganizing"
                            },
                            {
                                "type": "test",
                                "section": "Testing"
                            }
                        ]
                    }
                }
            ],
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "docs/CHANGELOG.md"
                }
            ],
            "@semantic-release/npm",
            [
                "@semantic-release/exec",
                {
                    "publishCmd": "zip dist.zip ./dist/* -r"
                }
            ],
            [
                "@semantic-release/github",
                {
                    "assets": [
                        {
                            "path": "dist.zip",
                            "label": "Distribution Files"
                        }
                    ]
                }
            ]
        ],
        "branches": [
            {
                "name": "main",
                "prerelease": false
            },
            {
                "name": "next",
                "prerelease": "next"
            },
            {
                "name": "beta",
                "prerelease": "beta"
            }
        ]
    }
}
